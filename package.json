{
  "name": "swapi",
  "version": "0.0.1",
  "description": "Swapi Star Wars API",
  "main": "index.js",
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "nyc": {
    "all": false,
    "include": [
      "src/**"
    ],
    "exclude": [
      "**/migrations/**",
      "src/config/**"
    ]
  },
  "scripts": {
    "pretest": "npm run migrate-test:down && npm run migrate-test:up",
    "test": "cross-env SWAPI_NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=text-summary mocha -c -r esm \"./tests/**/*.js\" --timeout 12000 --exit",
    "posttest": "npm run migrate-test:down",
    "prestart": "npm run migrate:up",
    "start": "cross-env SWAPI_NODE_ENV=production node -r esm .",
    "dev": "nodemon -r esm .",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down -c 100",
    "migrate-test:up": "db-migrate up --config database.json -e test",
    "migrate-test:down": "db-migrate down -c 100 --config database.json -e test",
    "lint": "eslint . --ext .js ./",
    "lint-fix": "eslint . --ext .js ./ --fix"
  },
  "keywords": [
    "management"
  ],
  "author": "Enyata <devs@enyata.com>",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "app-root-path": "^3.0.0",
    "axios": "^0.24.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "db-migrate": "^0.11.9",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "file-stream-rotator": "^0.5.7",
    "fs": "0.0.1-security",
    "helmet": "^3.23.3",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.3.2",
    "morgan": "^1.10.0",
    "nock": "^13.2.0",
    "node-cmd": "^4.0.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "pg-promise": "^10.4.4",
    "prettier": "^2.2.1",
    "request-ip": "^2.1.3",
    "supertest": "^2.0.1",
    "winston": "^2.3.0",
    "winston-daily-rotate-file": "^4.5.0",
    "winston-logstash": "^0.4.0",
    "winston-logstash-transport": "^2.0.0"
  }
}
